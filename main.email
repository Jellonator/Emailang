!bar;
!<stack@bar> {
	"^init$" {
		data = (,);
	}
	"^push$" {
		data = @data + (@content) + (@attachments);
	}
	"^print$" {
		print + @data > <io@std.com>;
	}
	"^empty$" {

	}
	"^pop$" {
		(@content, @data[-1]) > @sender;
		data = @data[0:-1];
	}
};

init > <stack@bar>;

!<user@bar> {
	"^start$" {
		(eq, get_pop, foo, bar) > <cmp@std.com>;
		(eq, get_pop, bar, bar) > <cmp@std.com>;
	}
	"^get_pop$" {
		(print, "Popped: ", @content) > <io@std.com>;
	}
};

"start" > <user@bar>;
